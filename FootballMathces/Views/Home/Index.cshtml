@model IEnumerable<FootballMathces.Models.Team>
@{
    ViewData["Title"] = "Home Page";
}

<p>
    <a asp-action="Create">Create Sample</a>
</p>

<table class="table">
    <thead>
        <tr>
            <th>
                id
            </th>
            <th>
                name
            </th>
            <th>
                desc
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.Id
                </td>
                <td>
                    @item.Name
                </td>
                <td>
                    @item.Desc
                </td>
            </tr>
        }
    </tbody>
</table>
